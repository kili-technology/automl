[flake8]
; Whitespace before ':' (E203)
; Line break occurred before a binary operator (W503)
; U101 - An unused argument starting with an underscore
; B006 Do not use mutable data structures for argument defaults
ignore = E203, W503, U101, B006
max-line-length = 100
exclude = tests,kiliautoml/utils/ultralytics/yolov5,kiliautoml/utils/label_errors/yolo_metrics.py,kiliautoml/utils/label_errors/yolo_general.py

unused-arguments-ignore-abstract-functions = True
unused-arguments-ignore-stub-functions = True


; Module level import not at top of file E402
per-file-ignores =
    main.py:E402
